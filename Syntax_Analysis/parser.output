Grammar

    0 $accept: program "end of file"

    1 program: header declarations subprograms comp_statement "."

    2 header: "program" "id" ";"
    3       | error "program" "id" ";"
    4       | "program" error "id" ";"
    5       | "program" "id" error ";"

    6 declarations: constdefs typedefs vardefs

    7 constdefs: "const" constant_defs ";"
    8          | "const" constant_defs error
    9          | ε

   10 constant_defs: constant_defs ";" "id" "=" expression
   11              | "id" "=" expression

   12 expression: expression "> or >= or < or <= or <>" expression
   13           | expression "=" expression
   14           | expression "IN" expression
   15           | expression "OR" expression
   16           | expression "+ or -" expression
   17           | expression "* or / or DIV or AND or MOD" expression
   18           | "+ or -" expression
   19           | "NOT" expression
   20           | variable
   21           | "id" "(" expressions ")"
   22           | "length" "(" expression ")"
   23           | "new" "(" expression ")"
   24           | constant
   25           | "(" expression ")"
   26           | "(" expression error
   27           | setlistexpression

   28 variable: "id"
   29         | variable "." "id"
   30         | variable "[" expressions "]"
   31         | "listfunc" "(" expression ")"
   32         | "listfunc" "(" expression error

   33 expressions: expressions "," expression
   34            | expression

   35 constant: "integer constant"
   36         | "real constant"
   37         | "boolean constant"
   38         | "char constant"

   39 setlistexpression: "[" expressions "]"
   40                  | "[" "]"

   41 typedefs: "type" type_defs ";"
   42         | ε

   43 type_defs: type_defs ";" "id" "=" type_def
   44          | "id" "=" type_def

   45 type_def: "array" "[" dims "]" "of" typename
   46         | "array" "[" dims error "of" typename
   47         | "list" "of" typename
   48         | "set" "of" typename

   49 $@1: ε

   50 $@2: ε

   51 type_def: "record" $@1 fields $@2 "end"
   52         | limit ".." limit

   53 dims: dims "," limits
   54     | limits

   55 limits: limit ".." limit
   56       | "id"

   57 limit: sign "integer constant"
   58      | "char constant"
   59      | "boolean constant"
   60      | "+ or -" "id"
   61      | "id"

   62 sign: "+ or -"
   63     | ε

   64 typename: standard_type
   65         | "id"

   66 standard_type: "integer"
   67              | "real"
   68              | "boolean"
   69              | "char"

   70 fields: fields ";" field
   71       | field

   72 field: identifiers ":" typename

   73 identifiers: identifiers "," "id"
   74            | "id"

   75 vardefs: "var" variable_defs ";"
   76        | ε

   77 variable_defs: variable_defs ";" identifiers ":" typename
   78              | identifiers ":" typename

   79 subprograms: subprograms subprogram ";"
   80            | ε

   81 subprogram: sub_header ";" "forward"
   82           | sub_header ";" declarations subprograms comp_statement

   83 sub_header: "function" "id" formal_parameters ":" standard_type
   84           | "function" "id" formal_parameters ":" "list"
   85           | "procedure" "id" formal_parameters
   86           | "function" "id"
   87           | "function" error "id" formal_parameters ":" standard_type
   88           | "function" "id" formal_parameters error ":" standard_type

   89 formal_parameters: "(" parameter_list ")"
   90                  | ε

   91 parameter_list: parameter_list ";" pass identifiers ":" typename
   92               | pass identifiers ":" typename

   93 pass: "var"
   94     | ε

   95 comp_statement: "begin" statements "end"

   96 statements: statements ";" statement
   97           | statement

   98 statement: assignment
   99          | if_statement
  100          | case_statement
  101          | while_statement
  102          | for_statement
  103          | with_statement
  104          | subprogram_call
  105          | io_statement
  106          | comp_statement
  107          | ε

  108 assignment: variable ":=" expression
  109           | variable ":=" "string"

  110 $@3: ε

  111 $@4: ε

  112 if_statement: "if" expression "then" $@3 statement $@4 if_tail

  113 $@5: ε

  114 if_tail: "else" $@5 statement
  115        | ε

  116 case_statement: "case" expression "of" cases case_tail "end"

  117 cases: cases ";" single_case
  118      | single_case

  119 single_case: label_list ":" statement
  120            | ε

  121 label_list: label_list "," label
  122           | label

  123 label: sign constant
  124      | sign "id"

  125 case_tail: ";" "otherwise" ":" statement
  126          | ε

  127 $@6: ε

  128 while_statement: "while" expression "do" $@6 statement

  129 $@7: ε

  130 for_statement: "for" "id" ":=" iter_space "do" $@7 statement

  131 iter_space: expression "to" expression
  132           | expression "downto" expression

  133 $@8: ε

  134 with_statement: "with" variable "do" $@8 statement

  135 subprogram_call: "id"
  136                | "id" "(" expressions ")"

  137 io_statement: "read" "(" read_list ")"
  138             | "write" "(" write_list ")"

  139 read_list: read_list "," read_item
  140          | read_item

  141 read_item: variable

  142 write_list: write_list "," write_item
  143           | write_item

  144 write_item: expression
  145           | "string"


